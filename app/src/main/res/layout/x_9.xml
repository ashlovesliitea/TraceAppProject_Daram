<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/x_9"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:outlineProvider="background"
    android:background="@drawable/x_9"
    >

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="match_parent"
        android:layout_height="306dp"
        android:background="@drawable/x9_rectangle_3">
    </View>

    <ImageView
        android:id="@+id/ellipse_22"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:contentDescription="@string/X_9_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="167dp"
        android:layout_marginStart="167dp"
        android:layout_marginTop="82dp"
        android:background="@drawable/x9_ellipse_22" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="168dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text="@string/X_9_id"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/X_9_order"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:layout_marginTop="400dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        />
    <View
        android:layout_width="350dp"
        android:layout_height="2dp"
        android:background="@color/black"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="440dp"
        />
    <ListView
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="460dp"/>
</RelativeLayout>
