                <action android:name="android.intent.action.MAIN"/>
                <category android:name="no.nordicsemi.android.nrftoolbox.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="no.nordicsemi.android.blinky.BlinkyActivity"
            android:launchMode="singleTop"
            android:parentActivityName="no.nordicsemi.android.blinky.ScannerActivity">
        </activity>

    </application>

